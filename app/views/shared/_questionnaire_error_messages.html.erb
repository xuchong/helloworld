<% if @questionnaire.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error">
      The form contains <%= pluralize(@questionnaire.errors.count, "error") %>.
    </div>
    <ul>
    <% @questionnaire.errors.full_messages.each do |msg| %>
      <li>* <%= msg %></li>
    <% end %>
    </ul>
  </div>  
<% end %>
